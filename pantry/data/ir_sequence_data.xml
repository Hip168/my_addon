<odoo>
    <data noupdate="1">
        <!-- Sequence for Pantry Check -->
        <record id="seq_pantry_check" model="ir.sequence">
            <field name="name">Pantry Check</field>
            <field name="code">pantry.check</field>
            <field name="prefix">KK</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False" />
        </record>

        <!-- Sequence for Pantry Restock -->
        <record id="seq_pantry_restock" model="ir.sequence">
            <field name="name">Pantry Restock</field>
            <field name="code">pantry.restock</field>
            <field name="prefix">NH</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False" />
        </record>
    </data>
</odoo>